"VertexlitGeneric"
{
	"$basetexture" "models/allies/Trigger_Turret_DiffuseMap"
	"$bumpmap" "models/allies/Trigger_Turret_NormalsMap"

	"$model" 1
	"$phong" "1" //this turns on the phong effect
	"$phongexponent"  "5"
	"$phongboost" "5" //use this to increase/decrease the highlight intensity 
	"$phongfresnelranges" "[.2 .75 1]" //this are the highlight intensities at different incidence angles from the camera (first value is perpendicular to the camera and the 3rd value is away from the camera)

}
